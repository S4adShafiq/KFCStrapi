{
  "kind": "collectionType",
  "collectionName": "addons",
  "info": {
    "singularName": "addon",
    "pluralName": "addons",
    "displayName": "Addon"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "biginteger"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "metaTitle": {
      "type": "string"
    },
    "metaDesc": {
      "type": "text"
    },
    "food_items": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::food-item.food-item",
      "mappedBy": "addons"
    }
  }
}
