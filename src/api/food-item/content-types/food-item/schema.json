{
  "kind": "collectionType",
  "collectionName": "food_items",
  "info": {
    "singularName": "food-item",
    "pluralName": "food-items",
    "displayName": "FoodItem"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "text"
    },
    "price": {
      "type": "biginteger"
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "available": {
      "type": "boolean"
    },
    "metaTitle": {
      "type": "string"
    },
    "metaDesc": {
      "type": "text"
    },
    "addons": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::addon.addon",
      "inversedBy": "food_items"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category",
      "inversedBy": "food_item"
    }
  }
}
